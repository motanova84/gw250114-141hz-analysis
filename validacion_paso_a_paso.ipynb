    "def analizar_detector_gw150914(detector):\n",
    "    \"\"\"\n",
    "    AnÃ¡lisis completo de un detector para GW150914\n",
    "    \"\"\"\n",
    "    print(f\"\\nğŸ“¡ Analizando {detector} - GW150914\")\n",
    "    print(\"-\" * 40)\n",
    "    \n",
    "    try:\n",
    "        # Descargar datos (32 segundos alrededor del merger)\n",
    "        start = gps_gw150914 - 16\n",
    "        end = gps_gw150914 + 16\n",
    "        print(f\"   ğŸ”„ Descargando datos {start} - {end} GPS...\")\n",
    "        \n",
    "        data = ts.TimeSeries.fetch_open_data(\n",
    "            detector, start, end, sample_rate=4096\n",
    "        )\n",
    "        \n",
    "        print(f\"   âœ… Datos descargados: {len(data)} muestras\")\n",
    "        print(f\"   ğŸ“Š Sample rate: {data.sample_rate} Hz\")\n",
    "        \n",
    "        # Aislar el ringdown (50 ms post-merger)\n",
    "        merger_idx = int((gps_gw150914 - data.t0.value) * data.sample_rate.value)\n",
    "        ringdown_samples = int(0.05 * data.sample_rate.value)  # 50 ms\n",
    "        ringdown = data[merger_idx:merger_idx + ringdown_samples]\n",
    "        \n",
    "        print(f\"   ğŸ” Ringdown extraÃ­do: {len(ringdown)} muestras ({len(ringdown)/data.sample_rate.value*1000:.1f} ms)\")\n",
    "        \n",
    "        # AnÃ¡lisis espectral\n",
    "        spectrum = ringdown.asd(fftlength=None)\n",
    "        \n",
    "        # Encontrar pico mÃ¡s cercano a 141.7 Hz\n",
    "        freq_idx = np.argmin(np.abs(spectrum.frequencies.value - frecuencia_objetivo))\n",
    "        detected_freq = spectrum.frequencies.value[freq_idx]\n",
    "        power_detected = spectrum.value[freq_idx]\n",
    "        \n",
    "        # Calcular SNR\n",
    "        freq_mask = (spectrum.frequencies.value >= 130) & (spectrum.frequencies.value <= 160)\n",
    "        noise_floor = np.median(spectrum.value[freq_mask])\n",
    "        snr = power_detected / noise_floor\n",
    "        \n",
    "        print(f\"   ğŸ¯ Frecuencia detectada: {detected_freq:.2f} Hz\")\n",
    "        print(f\"   ğŸ“ˆ SNR calculado: {snr:.2f}\")\n",
    "        \n",
    "        # Comparar con expectativa\n",
    "        expected = resultados_esperados[detector]\n",
    "        freq_error = abs(detected_freq - expected['freq'])\n",
    "        snr_error = abs(snr - expected['snr'])\n",
    "        \n",
    "        print(f\"   ğŸ“Š Esperado: f={expected['freq']:.2f} Hz, SNR={expected['snr']:.2f}\")\n",
    "        print(f\"   ğŸ“ Error freq: {freq_error:.3f} Hz, Error SNR: {snr_error:.2f}\")\n",
    "        \n",
    "        # ValidaciÃ³n\n",
    "        freq_ok = freq_error < 0.2  # Tolerancia\n",
    "        snr_ok = snr_error < 3.0    # Tolerancia\n",
    "        \n",
    "        print(f\"   {'âœ…' if freq_ok else 'âŒ'} Frecuencia vÃ¡lida: {freq_ok}\")\n",
    "        print(f\"   {'âœ…' if snr_ok else 'âŒ'} SNR vÃ¡lido: {snr_ok}\")\n",
    "        \n",
    "        return {\n",
    "            'detector': detector,\n",
    "            'freq_detected': detected_freq,\n",
    "            'snr': snr,\n",
    "            'freq_valid': freq_ok,\n",
    "            'snr_valid': snr_ok,\n",
    "            'spectrum': spectrum,\n",
    "            'data': data\n",
    "        }\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"   âŒ Error analizando {detector}: {e}\")\n",
    "        return None\n",
    "\n",
    "# Analizar ambos detectores\n",
    "resultado_h1 = analizar_detector_gw150914('H1')\n",
    "resultado_l1 = analizar_detector_gw150914('L1')"
    "# âš™ï¸ Funciones de preprocesamiento\n",
    "def preprocess_data(data):\n",
    "    \"\"\"Preprocesamiento estÃ¡ndar LIGO\"\"\"\n",
    "    # Filtros estÃ¡ndar\n",
    "    data = data.highpass(20)  # Remover ruido de baja frecuencia\n",
    "    data = data.notch(60)     # Remover lÃ­nea de 60 Hz\n",
    "    data = data.notch(120)    # Remover armÃ³nico de 120 Hz\n",
